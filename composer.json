{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/tpv-ebben/PHPWord"
        }
    ],
    "require": {
        "php": ">=7.1.3",
        "aws/aws-php-sns-message-validator": "^1.9",
        "aws/aws-sdk-php": "3.7",
        "barryvdh/laravel-dompdf": "^0.8.3",
        "doctrine/dbal": "^2.8",
        "dompdf/dompdf": "^0.8.3",
        "emadadly/laravel-uuid": "^1.2",
        "fideloper/proxy": "^4.0",
        "fx3costa/laravelchartjs": "^2.5",
        "guzzlehttp/guzzle": "^6.3",
        "intervention/image": "^2.4",
        "laravel/framework": "^6.0",
        "laravel/tinker": "~1.0",
        "laravelcollective/html": "^6.4",
        "league/flysystem": "^1.0",
        "league/flysystem-aws-s3-v3": "^1.0",
        "league/flysystem-sftp": "^1.0",
        "league/oauth2-client": "^2.4",
        "morrislaptop/laravel-queue-clear": "^1.2",
        "mpdf/mpdf": "^8.0",
        "nesbot/carbon": "^2.0.0",
        "owen-it/laravel-auditing": "^9.0",
        "php-http/message-factory": "^1.1",
        "phpoffice/phpspreadsheet": "^1.4",
        "phpoffice/phpword": "dev-develop",
        "predis/predis": "^1.1",
        "pusher/pusher-php-server": "^5.0",
        "ramsey/uuid": "^3.7",
        "sentry/sentry-laravel": "^1.7",
        "setasign/fpdf": "^1.8",
        "setasign/fpdi": "^2.2",
        "spatie/image-optimizer": "^1.7",
        "spatie/laravel-image-optimizer": "^1.6",
        "stevenmaguire/oauth2-salesforce": "^2.0",
        "twilio/sdk": "6.11.0",
        "wildbit/postmark-php": "^4.0"
    },
    "require-dev": {
        "filp/whoops": "~2.0",
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~7.5"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/Helpers/global.php",
            "app/Helpers/global_permission_helpers.php",
            "app/Helpers/blade_directives.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "platform": {
            "php": "7.3.33"
        },
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}